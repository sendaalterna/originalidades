apper\"> -->\n    <nav class=\"navbar navbar-dropdown navbar-expand-lg\" mbr-class=\"{'navbar-fixed-top': sticky}\">\n        <div class=\"menu-container\" mbr-class=\"{'container': !fullWidth,\n\t\t\t\t\t\t'container-fluid mbr-px-2': fullWidth,\n\t\t\t\t\t\t'mbr-jc-e': !showLogo &amp;&amp; !showBrand}\">\n            <!-- SHOW LOGO -->\n            <div class=\"navbar-brand\" mbr-if=\"showLogo || showBrand\">\n                <div mbr-if=\"showLogo\" class=\"navbar-logo\">\n                    <amp-img src=\"@PROJECT_PATH@/assets/images/logo-fan-page-1-90x90.png\" layout=\"responsive\" width=\"60\" height=\"60\" alt=\"image\" class=\"mobirise-loader\">\n                        <div placeholder=\"\" class=\"placeholder\"></div>\n                        <a href=\"index.html#features1-6\"><img class=\"mbr-temp\" src=\"@PROJECT_PATH@/assets/images/logo-fan-page-1-90x90.png\" alt=\"\" title=\"\"></a>\n                    </amp-img>\n                </div>\n                <span mbr-if=\"showBrand\" mbr-buttons=\"\" mbr-theme-style=\"display-7\" class=\"navbar-caption-wrap\" data-toolbar=\"-mbrBtnMove,-mbrBtnAdd,-mbrBtnRemove\"><a class=\"navbar-caption  display-6 text-danger\" data-app-selector=\".navbar-caption\" href=\"index.html#features1-6\" data-app-placeholder=\"Type Text\">Originalidades</a></span>\n            </div>\n            <!-- SHOW LOGO END -->\n            <!-- COLLAPSED MENU -->\n            <div class=\"collapse navbar-collapse\" id=\"navbarSupportedContent\">\n                <button class=\"navbar-toggler\" type=\"button\" data-toggle=\"collapse\" data-target=\"#navbarSupportedContent\" aria-controls=\"navbarNavAltMarkup\" aria-expanded=\"false\" aria-label=\"Toggle navigation\" mbr-if=\"showItems || showSocial || showButtons\">\n                    <div class=\"hamburger\">\n                        <span></span>\n                        <span></span>\n                        <span></span>\n                        <span></span>\n                    </div>\n                </button>\n                <!-- NAVBAR ITEMS -->\n                <ul mbr-menu=\"\" class=\"navbar-nav nav-dropdown\" mbr-theme-style=\"display-4\" mbr-if=\"showItems\" mbr-class=\"{'nav-right': !showButtons,'navbar-nav-top-padding': isPublish &amp;&amp; !showBrand &amp;&amp; !showLogo}\"><li class=\"nav-item\">\n                        <a class=\"nav-link link text-white\" href=\"tienda.html#menu1-c\" data-app-selector=\".nav-link,.dropdown-item\" data-app-placeholder=\"Type Text\">Tienda</a>\n                    </li>\n                    \n                    \n                    <li class=\"nav-item\"><a class=\"nav-link link  text-white\" href=\"contacto.html#menu1-q\" data-app-selector=\".nav-link,.dropdown-item\" aria-expanded=\"false\" data-app-placeholder=\"Type Text\">Contáctanos</a></li>\n                    <li class=\"nav-item\"><a class=\"nav-link link  text-white\" href=\"index.html#features1-6\" data-app-selector=\".nav-link,.dropdown-item\" aria-expanded=\"false\" data-app-placeholder=\"Type Text\">Home</a></li></ul>\n                <!-- NAVBAR ITEMS END -->\n                <!-- SOCIAL ICON -->\n                <div class=\"menu-social-list\" mbr-if=\"showSocial\" mbr-list=\"\" mbr-list-grow=\"socialCount\" mbr-link-type=\"socials\">\n                    <a href=\"https://bit.ly/31t48A3\" target=\"_blank\">\n                        <span mbr-icon=\"\" class=\"amp-iconfont animation-normal fa-whatsapp fa\"></span>\n                    </a>\n\n\n\n\n\n                    <a href=\"https://instagram.originalidades\" target=\"_blank\">\n                        <span mbr-icon=\"\" class=\"amp-iconfont animation-normal fa-instagram fa\"></span>\n                    </a><a href=\"https://facebook.com/originalidades\" target=\"_blank\">\n                        <span mbr-icon=\"\" class=\"amp-iconfont animation-normal fa-facebook-square fa\"></span>\n                    </a></div>\n                <!-- SOCIAL ICON END -->\n       