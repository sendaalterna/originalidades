          "_name": "shop02",
          "_customHTML": "<section class=\"amp-shop shop2\" group=\"Shop\" plugins=\"amp-main, amp-lightbox, amp-bind\">\n    <mbr-parameters>\n        <!-- Block parameters controls (Blue \"Gear\" panel) -->\n        <header>Size</header>\n        <input type=\"range\" inline title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"10\" step=\"1\" value=\"5\">\n        <input type=\"range\" inline title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"10\" step=\"1\" value=\"2\">\n\n        <header>Show/Hide</header>\n        <input type=\"checkbox\" title=\"Main Title\" name=\"titleSection\">\n        <input type=\"checkbox\" name=\"showText\" title=\"Show Text\">\n        <header condition=\"customStyle\">Custom Styles</header>\n        <input type=\"checkbox\" name=\"customStyles\" title=\"Custom Style\" checked>\n        <header condition=\"customStyles\">Card Color</header>\n        <input type=\"checkbox\" name=\"useColorCard\" title=\"Use Background Card\" condition=\"customStyles\" checked>\n        <input type=\"color\" name=\"colorCard\" title=\"Color\" value=\"#f5f5f5\" condition=\"useColorCard && customStyles\">\n        <input type=\"range\" title=\"Opacity\" name=\"opacityCard\" min=\"0\" max=\"1\" step=\"0.1\" value=\"1\" condition=\"useColorCard && customStyles\">\n        <input type=\"color\" name=\"fontColor\" title=\"Font Color\" value=\"#4a4a4a\" condition=\"customStyles\">\n        <input type=\"color\" name=\"btnColor\" title=\"Btn Color\" value=\"#232323\" condition=\"customStyles\">\n        <input type=\"color\" name=\"btnText\" title=\"Btn Text Color\" value=\"#ffffff\" condition=\"customStyles\">\n\n        <header condition=\"customStyles\">Paddings</header>\n        <input type=\"checkbox\" name=\"addPadding\" title=\"Add Padding\" condition=\"customStyles\" checked>\n        <input type=\"checkbox\" name=\"imgPadding\" title=\"No Image Padding\" condition=\"customStyles && addPadding\">\n        <input type=\"range\" title=\"X Padding\" name=\"xPadding\" min=\"0\" max=\"5\" step=\"0.5\" value=\"2\" condition=\"addPadding && customStyles\">\n        <input type=\"range\" title=\"Y Padding\" name=\"yPadding\" min=\"0\" max=\"5\" step=\"0.5\" value=\"2\" condition=\"addPadding && customStyles\">\n\n        <header condition=\"customStyles\">Box Shadow</header>\n        <input type=\"checkbox\" name=\"boxShadow\" title=\"Shadow\" condition=\"customStyles\" checked>\n        <input type=\"range\" name=\"shadowSize\" title=\"Size\" min=\"0\" max=\"100\" step=\"1\" value=\"48\" condition=\"customStyles && boxShadow\">\n        <input type=\"range\" name=\"shadowOpacity\" title=\"Opacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.3\" condition=\"customStyles && boxShadow\">\n        <input type=\"color\" name=\"shadowColor\" title=\"Color\" condition=\"customStyles && boxShadow\">\n\n        <header condition=\"customStyles\">Border Radius</header>\n        <input type=\"checkbox\" name=\"useBorderRadius\" title=\"Round Border\" condition=\"customStyles\" checked>\n        <input type=\"range\" name=\"borderRadius\" title=\"Size\" min=\"0\" max=\"50\" step=\"1\" value=\"24\" condition=\"customStyles && useBorderRadius\">\n        <header>Background</header>\n        <fieldset type=\"background\" name=\"bg\" required>\n            <input type=\"image\" title=\"Background Image\" value=\"../_images/background3.jpg\">\n            <input type=\"color\" title=\"Background Color\" value=\"#ffffff\" selected>\n        </fieldset>\n        <input type=\"checkbox\" title=\"Overlay\" name=\"overlay\" checked condition=\"bg.type !== 'color'\">\n        <input type=\"color\" title=\"Overlay Color\" name=\"overlayColor\" value=\"#efefef\" condition=\"overlay && bg.type == 'image'\">\n        <input type=\"range\" inline title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.5\" condition=\"overlay && bg.type == 'image'\">\n    </mbr-parameters>\n    <div class=\"mbr-overlay\" mbr-if=\"overlay && bg.type !== 'color'\"></div>\n    <div class=\"container-fluid\">\n        <div class=\"amp-shop\">\n            <h2 mbr-text mbr-theme-style=\"display-2\" class=\"amp-shop__title mbr-pb-5 mbr-fonts-style\" mbr-if=\"titleSection\">Our Shop</h2>\n            <div class=\"mbr-row mbr-jc-c\">\n                <!--1-->\n\n\n                <div on=\"tap:showLightbox4\" role=\"button\" tabindex=\"0\" class=\"item shop-image mbr-col-lg-3 mbr-col-md-6 mbr-col-sm-12 mbr-pb-5\">\n                    <!--Ligtbox-->\n                    <amp-lightbox id=\"showLightbox1\" layout=\"nodisplay\">\n                        <div class=\"lightbox\">\n                            <div class=\"item-box mbr-row\">\n                                <button type=\"button\" class=\"close\" aria-label=\"Close\" on=\"tap:showLightbox3.close\">\n                                    <span aria-hidden=\"true\"></span>\n                                </button>\n                                <div class=\"item-box-img mbr-col-lg-6 mbr-col-md-12 mbr-col-sm-12 mbr-flex mbr-p-4\">\n                                    <amp-img src=\"@PROJECT_PATH@/assets/images/captura-499x584.png\" layout=\"responsive\" width=\"341.7808219178082\" height=\"400\" alt=\"image\" class=\"placeholder-loader\">\n                                        <div placeholder class=\"placeholder\"></div>\n                                        <img class=\"mbr-temp\" src=\"@PROJECT_PATH@/assets/images/captura-499x584.png\" alt title>\n                                    </amp-img>\n                                </div>\n                                <div class=\"item-box-wrapper mbr-col-lg-6 mbr-col-md-12 mbr-col-sm-12 mbr-p-4\">\n\n                                    <div class=\"item-box-header\">\n                                        <h4 class=\"item-box-title item-box-title1 mbr-bold mbr-fonts-style mbr-pb-1\" mbr-theme-style=\"display-4\" data-app-selector=\".item-box-title1\">Escritorio&nbsp;</h4>\n             